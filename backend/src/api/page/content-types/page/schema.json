{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "name": "page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "shortName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "contentSections": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "sections.hero",
        "sections.large-video",
        "sections.lead-form",
        "sections.news-room",
        "sections.industry-sectors",
        "sections.hero-simple",
        "sections.client-marquee",
        "sections.banner",
        "sections.about-company",
        "sections.services",
        "sections.vision-mission",
        "sections.award-certification",
        "sections.associations",
        "sections.teams",
        "shared.map-embed",
        "sections.faq",
        "sections.services-grid",
        "sections.service-value",
        "sections.large-image",
        "sections.blog-content",
        "sections.jargon-slider",
        "sections.career-benefit",
        "sections.job-slider",
        "sections.job-explore"
      ]
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "targetField": "shortName",
      "required": true
    },
    "heading": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "navbarTheme": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "default",
      "enum": [
        "default",
        "white",
        "transparent"
      ]
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    }
  }
}
